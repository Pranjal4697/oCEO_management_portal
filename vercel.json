{
    "version": 2,
    "builds": [
      {
        "src": "run.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "run.py"
      }
    ],
    "env": {
      "FLASK_ENV": "production"
    },
    "functions": {
      "run.py": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }
  